{
  "name": "trial",
  "authors": [
    "Szabo Bogdan"
  ],
  "description": "The extensible test runner for DLang",
  "copyright": "Copyright Â© 2017, Szabo Bogdan",
  "license": "MIT",
  "homepage": "http://trial.szabobogdan.com/",
  "dependencies": {},
  "configurations": [
    {
      "name": "executable"
    },
    {
      "name": "trial",
      "excludedSourceFiles": [
        "source/app.d",
        "runner/app.d"
      ],
      "sourcePaths": [
        "lifecycle",
        "runner",
        "tests"
      ],
      "stringImportPaths": [
        "lifecycle/trial"
      ],
      "importPaths": [
        "lifecycle",
        "runner",
        "tests"
      ],
      "dependencies": {
        "dub": "~>1.8.0",
        "fluent-asserts": "~>0.9.0",
        "libdparse": "*"
      }
    },
    {
      "name": "unittest",
      "excludedSourceFiles": [
        "source/app.d",
        "runner/app.d"
      ],
      "sourcePaths": [
        "lifecycle",
        "runner",
        "tests"
      ],
      "stringImportPaths": [
        "lifecycle/trial"
      ],
      "importPaths": [
        "lifecycle",
        "runner",
        "tests"
      ],
      "dependencies": {
        "dub": "~>1.8.0",
        "fluent-asserts": "~>0.9.0",
        "libdparse": "*"
      }
    }
  ],
  "subPackages": [
    {
      "name": "lifecycle",
      "targetType": "library",
      "sourcePaths": [
        "lifecycle"
      ],
      "importPaths": [
        "lifecycle"
      ],
      "dependencies": {
        "fluent-asserts": {
          "version": "~>0.9.0",
          "optional": true
        },
        "libdparse": {
          "version": "*",
          "optional": true
        }
      }
    },
    {
      "name": "runner",
      "targetType": "executable",
      "targetName": "trial",
      "sourcePaths": [
        "runner"
      ],
      "importPaths": [
        "runner"
      ],
      "stringImportPaths": [
        "lifecycle/trial"
      ],
      "dependencies": {
        "dub": "~>1.8.0",
        "trial:lifecycle": "*",
        "libdparse": "*",
        "fluent-asserts:core": "~>0.9.0"
      }
    }
  ]
}
